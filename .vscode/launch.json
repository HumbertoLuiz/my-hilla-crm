    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "java",
            "name": "Launch Application",
            "request": "launch",
            "mainClass": "com.example.application.Application"
        },
        {
            "type": "java",
            "request": "attach",
            "name": "Attach to Application started using Maven",
            "hostName": "localhost",
            "port": 8080
        },
        {
            "type": "java",
            "name": "Spring Boot-Application<hilla-crm>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "mainClass": "com.example.application.Application",
            "projectName": "hilla-crm",
            "args": "",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}
